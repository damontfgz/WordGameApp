<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.gatech.seclass.wordfind6300">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlayGameActivity"></activity>
        <activity android:name=".WordWeightActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".GameScoreActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScoreStatActivity" />
        <activity android:name=".WordStatActivity" />
        <activity android:name=".GameStatsActivity" />
        <activity android:name=".StatisticsActivity" />
    </application>

</manifest>